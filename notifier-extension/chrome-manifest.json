{
  "manifest_version": 2,
  "version": "1.0.0",
  "short_name": "Online Notifier v2",
  "name": "Online Notifier v2",
  "offline_enabled": false,
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "64": "icon-64x64.png",
    "128": "icon-128x128.png"
  },
